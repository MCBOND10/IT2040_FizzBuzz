//This program is a usual FizzBuzz program. Print Fizz for nums % 3. Print Buzz for nums % 5. Print FizzBuzz for nums % 3 & 5.
//This program runs for nums 1-100.

#include <stdio.h>

int main(void)
{
    int i;
    
    for(i=1,i<100,i++)
    {
        if(((i%3)||(i%5))== 0)
            printf("FizzBuzz"\n);
        else if((i%3)==0)
            printf("Fizz\n");
        else if((i%5)==0)
            printf("Buzz\n");
        else
            printf("num= %d\n",i);
    }
    
    return 0; 
}
